{
  "name": "agilehub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server/index.js",
    "build:server": "webpack --mode production",
    "build": "rm -rf dist && next build && npm run build:server && cp -R .next dist && cp -R public dist && cp package.json dist && cp package-lock.json dist",
    "build:win": "rimraf dist && next build && npm run build:server && xcopy /i /e .next dist\\.next && xcopy /i /e public dist\\public && copy package.json dist && copy package-lock.json dist",
    "start": "cross-env NODE_ENV=production node server.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@next/env": "^14.2.5",
    "axios": "^1.7.4",
    "cookie": "^0.6.0",
    "express": "^4.19.2",
    "html-webpack-plugin": "^5.6.0",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "mongoose": "^8.5.3",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.5",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1"
  }
}

{
  "name": "agilehub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && rm -rf dist-server && npm run build:all && npm run generate:packagejson",
    "build:all": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "lint": "vue-cli-service lint",
    "build:client": "vue-cli-service build",
    "build:server": "cd server && tsc && cd ../ && webpack --mode production && rm -rf dist-server",
    "build:clean": "rm -rf dist",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vue-cli-service serve --port 3000",
    "dev:server": "cross-env nodemon --watch server --exec ts-node ./server/index.ts",
    "start": "cross-env nodemon dist/index.js",
    "generate:packagejson": "echo '{ \"scripts\": { \"start\": \"cross-env nodemon index.js\" } }' >> dev_package.json && mv dev_package.json dist/package.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "axios": "^1.7.5",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.8.3",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.4",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jquery": "^3.5.30",
    "@types/js-cookie": "^3.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.5.1",
    "@types/webpack-env": "^1.18.5",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "express": "^4.19.2",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.41",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "tailwindcss": "^3.4.10",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.5",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
